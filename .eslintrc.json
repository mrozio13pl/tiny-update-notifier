{
    "extends": "@mrozio/eslint-config/typescript",
    "plugins": ["spellcheck"],
    "rules": {
        /** Change it whenever you'd like to look for misspellings */
        "spellcheck/spell-checker": 0, 
        "consistent-return": 0,
        "max-len": 0,
        "unicorn/prefer-number-properties": 0
    },
    "parserOptions": {
        "project": "./tsconfig.json"
    }
}
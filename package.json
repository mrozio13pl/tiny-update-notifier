{
  "name": "tiny-update-notifier",
  "version": "0.0.0",
  "description": "Simple check for npm package updates",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": "mrozio13pl/tiny-update-notifier",
  "author": "mrozio13pl",
  "license": "MIT",
  "scripts": {
    "build": "node build.mjs",
    "dev": "npx cross-env TINY_UPDATE_NOTIFIER_DEBUG=true node demo.mjs"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "update",
    "package",
    "version",
    "npm"
  ],
  "devDependencies": {
    "@mrozio/eslint-config": "^1.0.1",
    "@release-it/conventional-changelog": "^7.0.2",
    "@types/semver": "^7.5.4",
    "cachedir": "^2.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "release-it": "^16.2.1",
    "semver": "^7.5.4",
    "terser": "^5.22.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "packageManager": "^pnpm@8.9.0"
}

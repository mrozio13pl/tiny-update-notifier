{
  "name": "tiny-update-notifier",
  "version": "1.0.1",
  "description": "Simple check for npm package updates",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": "mrozio13pl/tiny-update-notifier",
  "author": "mrozio13pl",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "npx cross-env TINY_UPDATE_NOTIFIER_DEBUG=true node demo.mjs",
    "publish": "clean-publish",
    "release": "npm run build && npx release-it && npm run publish"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "update",
    "package",
    "version",
    "npm"
  ],
  "devDependencies": {
    "@mrozio/eslint-config": "^1.0.3",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/node": "^20",
    "@types/semver": "^7.5.6",
    "cachedir": "^2.4.0",
    "clean-publish": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "release-it": "^17.0.0",
    "semver": "^7.5.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "packageManager": "^pnpm@8.9.0"
}
